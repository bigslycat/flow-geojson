{
  "name": "flow-geojson",
  "version": "1.0.2",
  "description": "Flow type definitions for GeoJSON",
  "keywords": [
    "GeoJSON",
    "Flow",
    "flowtype",
    "map"
  ],
  "main": "lib/GeoJSON.js",
  "files": [
    "README.md",
    "LICENSE.md",
    "lib"
  ],
  "scripts": {
