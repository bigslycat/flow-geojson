{
  "name": "flow-geojson",
  "version": "1.0.1",
  "description": "Flow type definitions for GeoJSON",
  "keywords": [
    "GeoJSON",
    "Flow",
    "flowtype",
    "map"
  ],
  "main": "lib/GeoJSON.js",
  "files": [
    "README.md",
    "LICENSE.md",
    "lib"
  ],
  "scripts": {
    "lint": "eslint .",
    "flow": "flow",
    "prebuild": "yarn run lint && yarn run flow",
    "build": "shx rm -rf lib && babel-node build",
