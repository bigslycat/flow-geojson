  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "eslint": "^4.13.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-flowtype": "^2.40.1",
    "eslint-plugin-import": "^2.8.0",
    "flow-bin": "^0.61.0",
    "husky": "^0.14.3"
  },
  "repository": "git@github.com:bigslycat/flow-geojson.git",
  "author": {
